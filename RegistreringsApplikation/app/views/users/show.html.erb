<%= @user.name %>, API KEY = <%= @user.key %>

      
      <% if is_logged_in? %>
      <div class="alert alert-success" role = "alert">Logged in as <%= current_user.name %> </div>
      <div class="alert alert-danger" role = "alert"><%= link_to "Delete API Key", @user, method: :delete %></div> <br>
      <div class="alert alert-info"role = "alert"> <%= link_to "Generate API Key", @user, method: :post %></div>
      <% end %>